  "isomorphic": {
    "assets": {
      "/(*/):name.js": {
        "output": "/assets/:name.bundle.js",
        "public": "/assets/:name.bundle.js",
        "transform": "isomorphic/compile/babel"
       },
       "/(*/):name.less": {
         "output": "/assets/:name.less",
         "public": "/assets/styles/:names.css",
         "transform": "isomorphic/compile/less"
      }
    }
  },
  "dependencies": {
    "@njudah/builder": "^12.0.0-beta.3",
    "babel-preset-es2015": "^6.24.1",
